{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, useEffect, useContext } from \"react\";\nimport NavLinks from \"./NavLinks\";\nimport LogoImage from \"../assets/images/logo.png\";\nimport { MenuContext } from \"../context/MenuContext\";\n\nconst HeaderHome = props => {\n  const {\n    0: sticky,\n    1: setSticky\n  } = useState(false);\n  const {\n    menuStatus,\n    updateMenuStatus\n  } = useContext(MenuContext);\n\n  const handleScroll = () => {\n    if (window.scrollY > 70) {\n      setSticky(true);\n    } else if (window.scrollY < 70) {\n      setSticky(false);\n    }\n  };\n\n  const handleMenuClick = e => {\n    e.preventDefault();\n    updateMenuStatus(!menuStatus);\n  };\n\n  useEffect(() => {\n    window.addEventListener(\"scroll\", handleScroll);\n    return () => {\n      window.removeEventListener(\"scroll\", handleScroll);\n    };\n  }, [sticky]);\n  return /*#__PURE__*/_jsx(\"header\", {\n    className: `site-header-one stricky  ${props.extraClassName} ${sticky === true ? \"stricky-fixed stricked-menu\" : \" \"}`,\n    children: /*#__PURE__*/_jsxs(\"div\", {\n      className: \"container-fluid\",\n      children: [/*#__PURE__*/_jsxs(\"div\", {\n        className: \"site-header-one__logo\",\n        children: [/*#__PURE__*/_jsx(\"a\", {\n          href: \"/\",\n          children: /*#__PURE__*/_jsx(\"img\", {\n            src: LogoImage,\n            width: \"98\",\n            alt: \"\"\n          })\n        }), /*#__PURE__*/_jsx(\"span\", {\n          className: \"side-menu__toggler\",\n          onClick: handleMenuClick,\n          children: /*#__PURE__*/_jsx(\"i\", {\n            className: \"fa fa-bars\"\n          })\n        })]\n      }), /*#__PURE__*/_jsx(\"div\", {\n        className: \"main-nav__main-navigation\",\n        children: /*#__PURE__*/_jsx(NavLinks, {})\n      })]\n    })\n  });\n};\n\nexport default HeaderHome;","map":null,"