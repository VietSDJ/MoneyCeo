{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, useEffect, useContext } from \"react\";\nimport NavLinks from \"./NavLinks\";\nimport LogoImage from \"../assets/images/logo.png\";\nimport { MenuContext } from \"../context/MenuContext\";\n\nvar HeaderHome = function HeaderHome(props) {\n  var _useState = useState(false),\n      sticky = _useState[0],\n      setSticky = _useState[1];\n\n  var _useContext = useContext(MenuContext),\n      menuStatus = _useContext.menuStatus,\n      updateMenuStatus = _useContext.updateMenuStatus;\n\n  var handleScroll = function handleScroll() {\n    if (window.scrollY > 70) {\n      setSticky(true);\n    } else if (window.scrollY < 70) {\n      setSticky(false);\n    }\n  };\n\n  var handleMenuClick = function handleMenuClick(e) {\n    e.preventDefault();\n    updateMenuStatus(!menuStatus);\n  };\n\n  useEffect(function () {\n    window.addEventListener(\"scroll\", handleScroll);\n    return function () {\n      window.removeEventListener(\"scroll\", handleScroll);\n    };\n  }, [sticky]);\n  return /*#__PURE__*/_jsx(\"header\", {\n    className: \"site-header-one stricky  \".concat(props.extraClassName, \" \").concat(sticky === true ? \"stricky-fixed stricked-menu\" : \" \"),\n    children: /*#__PURE__*/_jsxs(\"div\", {\n      className: \"container-fluid\",\n      children: [/*#__PURE__*/_jsxs(\"div\", {\n        className: \"site-header-one__logo\",\n        children: [/*#__PURE__*/_jsx(\"a\", {\n          href: \"/\",\n          children: /*#__PURE__*/_jsx(\"img\", {\n            src: LogoImage,\n            width: \"98\",\n            alt: \"\"\n          })\n        }), /*#__PURE__*/_jsx(\"span\", {\n          className: \"side-menu__toggler\",\n          onClick: handleMenuClick,\n          children: /*#__PURE__*/_jsx(\"i\", {\n            className: \"fa fa-bars\"\n          })\n        })]\n      }), /*#__PURE__*/_jsx(\"div\", {\n        className: \"main-nav__main-navigation\",\n        children: /*#__PURE__*/_jsx(NavLinks, {})\n      })]\n    })\n  });\n};\n\nexport default HeaderHome;","map":null,"metadata":{},"sourceType":"module"}